{
    "version": 6,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 10,
        "patch": 0
    },
    "buildPresets": [
        {
            "name": "debug",
            "configurePreset": "debug",
            "cleanFirst": true
        }
    ],
    "configurePresets": [
        {
            "name": "debug",
            "displayName": "Debug",
            "architecture": {
                "strategy": "external",
                "value": "avr8"
            },
            "generator": "Unix Makefiles",
            "description": "Default debug profile.",
            "binaryDir": "${sourceDir}/build/debug",
            "toolchainFile": "${sourceDir}/cmake/avr-gcc-toolchain.cmake",
            "environment": {
                "DEVICE": "atmega8",
                "F_CPU": "16000000",
                "E_FUSE": "0xFF",
                "L_FUSE": "0xEF",
                "H_FUSE": "0xC9",
                "PROGRAMMER": "usbasp"
            }
        }
    ]
}